{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/check", "destination": "/src/api/check" },
    { "source": "/health", "destination": "/src/api/check" },
    { "source": "/reset", "destination": "/src/api/reset" }
  ],
  "crons": [
